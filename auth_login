[DEFAULT]
ip = net1-%d
port = 22
user_name = root 
user_passwd = teamsun
is_key = False
debug = True

[wsgi_server]
# 设置 wsgi 服务的 监听地址
# The IP Address of the network interface to for the admin
# service to listen on. (string value)
wsgi_host=0.0.0.0

# 设置 wsgi 服务的 监听端口
# The port number which the wsgi service listens on.
# (integer value)
wsgi_port=5000

# 
allow_agent = True

# 设置 debug 信息打印
# Print debugging output (set logging level to DEBUG instead
# of default WARNING level). (boolean value)
debug = False

[ssh_server]
# 设置要登入的 ssh 服务器的 名字  eg: vm200 | aix140 规则是 "str%d" % ip 地址最后一组
#ssh service ip address
ssh_server_alias = vm57|vm58|182vm|vm56|165vm|23vm|58vm|vm28|166vm|github50|vm22|vm222|vm200|vm24?aix140,aix212#aix172,aix38,aix20|vm19|vm31|aix58

[ssh_subnets]
## 输入 有可能要登入的 网段
net = 172.24.23.0/24 | 192.168.122.0/24

[ssh_passwords]
## 输入 有可能的 密码
user_passwd = teamsun | toor 

[ssh_usernames]
## 输入 有可能的 用户
user_name = root | git | love 

[ssh_ports]
## 输入 有可能的端口号
port = 22 | 2222 | 8888

